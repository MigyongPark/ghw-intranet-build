      <careersSubjectHomePage id="2413" key="b062fd43-deb2-492e-85eb-1919e2971dcf" parentID="1487" level="3" creatorID="2" sortOrder="1" createDate="2017-07-19T11:07:24" updateDate="2017-11-22T12:39:15" nodeName="Apprenticeships" urlName="apprenticeships" path="-1,1288,1487,2413" isDoc="" nodeType="2144" creatorName="BrianCarter" writerName="BrianCarter" writerID="2" template="2145" nodeTypeAlias="careersSubjectHomePage">
