            <careersCoursePage id="3833" key="d645f208-b700-4f81-92e1-38f5d2ba0a12" parentID="2365" level="6" creatorID="11" sortOrder="39" createDate="2017-10-10T11:00:20" updateDate="2017-12-04T15:39:23" nodeName="Rhythm of life" urlName="rhythm-of-life" path="-1,1288,1487,5124,2362,2365,3833" isDoc="" nodeType="2256" creatorName="JasmineVickers" writerName="BrianCarter" writerID="2" template="2255" nodeTypeAlias="careersCoursePage">
