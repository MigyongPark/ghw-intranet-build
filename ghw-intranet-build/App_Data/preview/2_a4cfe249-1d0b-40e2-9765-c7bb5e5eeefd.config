      <careersSubjectHomePage id="2699" key="47236602-c429-4179-9c12-502ccdea4fd8" parentID="1487" level="3" creatorID="2" sortOrder="2" createDate="2017-07-26T15:18:18" updateDate="2017-07-27T08:47:42" nodeName="Clinical Supervision (Stepping Up)" urlName="clinical-supervision-stepping-up" path="-1,1288,1487,2699" isDoc="" nodeType="2144" creatorName="BrianCarter" writerName="admin" writerID="0" template="2145" nodeTypeAlias="careersSubjectHomePage">
