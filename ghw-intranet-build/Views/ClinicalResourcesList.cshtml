@inherits Umbraco.Web.Mvc.UmbracoTemplatePage<ContentModels.ClinicalResourcesList>
@using ContentModels = Umbraco.Web.PublishedContentModels;
@{
	Layout = "NewIntranetMaster.cshtml";
}
@Html.Partial("atozButtons")
<!----- P A G E   H E A D I N G ------------- S T A R T ------->
<div class="cliHeader" id="zoneClinical">
<img id="zonePageIcon" src="/media/1074/zonepageiconclinical.png" />
<h1 id="zoneWhite">Clinical systems</h1>
</div>
<!----- P A G E   H E A D I N G ------------- E N D ------->
<h2>Link back to old list</h2>
	<p>If any of the links below fail to work, please click on this link back to the <a href="http://gwh-intranet/trust-wide/it-services/clinical-apps-and-services.aspx" target="_blank">old listing page</a>.</p>
	
	

	<!----CLINICAL RESOURCES GRID START----------------------------------------->
	<div class="cliGrid">

		@foreach (var item in CurrentPage.Children)
		{
			<a href="@item.linkURL" target="_blank">					
				<div id="cliSquare">
					<table cellpadding="0" cellspacing="0">
						<tr>
							<td id="cliIcon"><img src="@Umbraco.Media(@item.icon).Url"></td>
						</tr>
						<tr>
							<td id="cliText">@item.linkText</td>
						</tr>
					</table>
				</div>
			</a>
		}	

	</div>
	<!----CLINICAL RESOURCES GRID END--------------------------------------------->




</div>
<!----- P A G E   C O N T A I N E R --- E N D ----->


</body>
</html>